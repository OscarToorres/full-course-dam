package ejercicio01;

import java.net.URL;

public class FuncionalidadURL {

	URL url;
	
	public static void main(String[] args) {
		
		System.out.println("Constructor para protocolo + URL + puerto + directorio:");
		url = new URL("http","docs.oracle.com",80, "/javase/7");
		Visualizar(url);
		
	}
	
}
