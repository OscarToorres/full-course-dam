package ejercicio2;

public class Persona {
	
	private String nome;
	private String dni;
	private String localidade;
	private int tlf;
	
	public Persona(String nome, String dni, String localidade, int tlf) {
		super();
		this.nome = nome;
		this.dni = dni;
		this.localidade = localidade;
		this.tlf = tlf;
	}
	
	

}
